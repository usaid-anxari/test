{
  "name": "truetestify-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "nest start --watch",
    "build": "nest build",
    "build:lambda": "nest build",
    "prepackage:lambda": "npm run build",
    "start": "node dist/main.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate:run": "psql postgres://postgres:Password123@localhost:5432/masterdb-02 -f migrations/001-create-milestone1.sql",
    "seed:auth0": "ts-node src/seed/runSeed.ts",
    "pre-deploy-check": "./scripts/pre-deploy-check.sh",
    "deploy:lambda": "npm run build:lambda && npx serverless@3.38.0 deploy",
    "deploy:lambda:prod": "npm run build:lambda && serverless deploy --stage prod",
    "package:lambda": "npm run build:lambda && serverless package",
    "offline": "serverless offline",
    "logs": "serverless logs -f api --tail",
    "remove:lambda": "serverless remove"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.5.2",
    "@types/nodemailer": "^7.0.1",
    "@types/pdfkit": "^0.17.3",
    "auth0": "^5.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.8.1",
    "dotenv": "^16.0.0",
    "express-jwt": "^8.5.1",
    "express-oauth2-jwt-bearer": "^1.7.1",
    "fast-safe-stringify": "^2.1.1",
    "googleapis": "^160.0.0",
    "iterare": "^1.2.1",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.10.0",
    "serverless-http": "^3.2.0",
    "stripe": "^18.5.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.18",
    "uid": "^2.0.2",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "serverless-domain-manager": "^8.0.0",
    "serverless-esbuild": "^1.55.2"
  }
}
